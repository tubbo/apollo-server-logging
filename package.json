{
  "name": "apollo-server-logging",
  "version": "1.0.1",
  "license": "MIT",
  "packageManager": "yarn@3.2.3",
  "repository": "https://github.com/tubbo/apollo-server-logging.git",
  "peerDependencies": {
    "graphql": "^16",
    "pino": "^8"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@jest/globals": "29.0.3",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@swc/core": "1.3.1",
    "@swc/jest": "0.2.22",
    "@types/eslint": "8.4.6",
    "@types/jest": "29.0.3",
    "@types/luxon": "3.0.1",
    "@types/node": "18.7.13",
    "@types/prettier": "2.7.0",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "apollo-server-plugin-base": "3.6.2",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jest": "27.0.4",
    "husky": "8.0.1",
    "jest": "29.0.3",
    "jest-mock": "29.0.3",
    "lint-staged": "13.0.3",
    "npm-run-all": "4.1.5",
    "pino": "8.5.0",
    "prettier": "2.7.1",
    "semantic-release": "19.0.5",
    "ts-node": "10.9.1",
    "typedoc": "0.23.14",
    "typescript": "4.8.3"
  },
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/esm/*.js",
      "require": "./dist/cjs/*.js"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "run-p build:types build:esm build:cjs docs",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationMap --outDir dist/types",
    "build:esm": "tsc --module esnext --target esnext --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es2016 --outDir dist/cjs",
    "docs": "typedoc",
    "types": "tsc --noEmit",
    "test": "node --no-warnings --experimental-vm-modules $(yarn bin jest)",
    "lint": "eslint .",
    "style": "prettier --check .",
    "fmt": "prettier --write . && eslint . --fix",
    "clean": "rm -rf dist",
    "publish": "HUSKY=0 semantic-release"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "prettier",
      "plugin:@typescript-eslint/recommended"
    ],
    "overrides": [
      {
        "files": [
          "**/*.test.*"
        ],
        "plugins": [
          "jest"
        ],
        "extends": [
          "plugin:jest/recommended"
        ]
      }
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "moduleNameMapper": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-typescript"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix",
      "yarn test -uo --pass-with-no-tests"
    ],
    "*.{md,json,yml,yaml,html,htm,css,graphql,gql}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/commit-analyzer",
        {
          "preset": "conventionalcommits"
        }
      ],
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "tarballDir": "pkg"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "docs",
            "package.json"
          ]
        }
      ],
      [
        "@semantic-release/github",
        {
          "assets": "pkg/*.tgz"
        }
      ]
    ]
  },
  "dependencies": {
    "luxon": "^3.0.2",
    "nanoid": "^4.0.0"
  }
}
